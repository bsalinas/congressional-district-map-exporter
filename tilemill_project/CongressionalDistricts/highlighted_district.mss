#districts { 
line-color:rgba(100,200,100,0.8);
  line-width:0.25; 
  [NAMELSAD= "State House District 18"]{ 
    polygon-fill:#45CB41; 
    polygon-opacity:0.5; 
    line-color:#000; 
    line-width:1.0; 
    text-name:"House18"; 
    text-face-name:@sans; 
    text-transform:uppercase; 
    text-character-spacing:1; 
    text-size:20; 
    text-allow-overlap:true; 
    text-halo-fill:rgba(255,255,255,0.75); 
    text-halo-radius:2; 
  } 
}